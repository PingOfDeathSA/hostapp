    <div class="row">
        <div class="col-sm-12" >
            <div style="text-align: center;">
                <h1>My Listed Items</h1>
              </div>
              
          
    
        </div>
    
      </div>





        <section class="main-items">
        <% Learn.forEach(function(item) { %>
          <div class="container-main-Card" >
            <h5 class="items" style="font-weight: bold; margin-left: 6%; font-size: 90%;">
              <%= item.Product_Name.length > 15 ? item.Product_Name.slice(0, 15) + '...' : item.Product_Name %>
            </h5>
            
              <div id="carouselExampleControls-<%= item._id %>" class="carousel slide" data-ride="carousel" style="height: 170px;  ">
                  <div class="carousel-inner">
                    <div class="carousel-item active">
                      
  
                 
                      <img class="ProductCardMain" style="display: inline;" data-toggle="modal" data-target="#popup<%= item._id %>" src="<%= item.imageProductMain %>" alt="First slide">
                    </div>
                    <div class="carousel-item">
                      <img class="ProductCardMain" style="display: inline;"  data-toggle="modal" data-target="#popup<%= item._id %>" src="<%= item.imageProduct3 %>" alt="Second slide">
                    </div>
                    <div class="carousel-item">
                      <img class="ProductCardMain" style="display: inline;"  data-toggle="modal" data-target="#popup<%= item._id %>" src="<%= item.imageProduct2 %>"  alt="Third slide">
                    </div>
                  </div>
                  <a class="carousel-control-prev" style="background-color: rgb(29, 28, 28); width: 25px; height: 15px; top: 50%; border-radius: 5px;" href="#carouselExampleControls-<%= item._id %>" role="button" data-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                  </a>
                  <a class="carousel-control-next" style="background-color: rgb(29, 28, 28); width: 25px; height: 15px; top: 50%; border-radius: 5px;" href="#carouselExampleControls-<%= item._id %>" role="button" data-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                  </a>
                </div>
            <!-- <p class="items" style="font-weight: 900; font-size:70%; margin-top:1%;margin-left:6%;margin-bottom:3%"> R <%= item.was_Price %></p> -->
         
  <p></p>
            <p style="margin: 0%;padding:0%;margin-bottom: 2%;"><span style="font-weight: 900; font-size:120%; margin-top:2%;margin-left:6%;" >R <%= item.price %>.00</span></p>
         
              

            <form action="/delete-item" method="POST">
              <span type="button" class="badge badge-secondary" style="box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);" data-toggle="modal" data-target="#popup<%= item._id %>">View</span>
            
              <!-- Additional button or input for the delete action -->
              <button type="submit" class="btn btn-danger" name="itemId" value="<%= item._id %>">Delete</button>
            </form>
            
               
              </div>
         
        </div>
        <% }); %>
      </section>
        
        
        
        <% Learn.forEach(function(item) { %>
            <div class="modal fade" id="popup<%= item._id %>" tabindex="-1" role="dialog" aria-labelledby="popup<%= item._id %>Label" aria-hidden="true">
                <div class="modal-dialog"  role="document">
                  <div class="modal-content">
                    <div class="row">
                      <div class="col-8"></div>
                      <div class="butto-n col-2" ><button type="button" class="btn btn-danger" data-dismiss="modal" aria-label="Close"> Close
                      </button></div>
                    </div>
                    
                    <div class="modal-header">
                      <div class="row">
                        <div class="col-auto">
                          <img class="ProductCard" style="display: inline;" data-toggle="modal" data-target="#popup<%= item._id %>" src="<%= item.imageProductMain %>" alt="">
                        </div>
                        <div class="col-auto"> 
                          <img class="ProductCard3" style="display: none;" data-toggle="modal" data-target="#popup<%= item._id %>" src="<%= item.imageProductMain %>" alt="">
                          <div class="product-details"> 
                            <p><%= item.details %></p>
                          </div>
                          <h5 class="items3" style="font-weight: bold;"><%= item.Product_Name %></h5>
                          <p  class="Price3" style="font-weight: 500; font-size:90%"> R <%= item.price %></p>
                       
                        </div>
                      </div>
                    </div>
                    <script>
          
                    </script>
                 
                    <div class="modal-body" style="height: 250px;">
    
                      <img class="ProductCard2" style="display: inline;" data-toggle="modal" data-target="#popup<%= item._id %>" src="<%= item.imageProduct1 %>" alt="">
                      <img class="ProductCard2" style="display: inline;"  data-toggle="modal" data-target="#popup<%= item._id %>" src="<%= item.imageProduct2 %>" alt="">
                      <img class="ProductCard2" style="display: inline;"  data-toggle="modal" data-target="#popup<%= item._id %>" src="<%= item.imageProduct3 %>" alt="">
                 
                    </div>
