<!DOCTYPE html>
<html>
<head>
  <title>All Images</title>
</head>
<body>
  <h1>All Images</h1>
  <% imagesData.forEach(function(image) { %>
    <div class="image-item">
      <h3><%= image.name %></h3>
      <% if (image.data) { %>
        <img src="data:<%= image.contentType %>;base64,<%= image.data.toString('base64') %>" alt="<%= image.name %>">
      <% } else { %>
        <p>No image data found for <%= image.name %></p>
      <% } %>
    </div>
  <% }); %>
  
</body>
</html>
